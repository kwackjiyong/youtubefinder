<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<typeAlias type="com.jiyong.yt.dto.TagDTO" alias="TagDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.LikeDTO" alias="LikeDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.UserDTO" alias="UserDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.PostDTO" alias="PostDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.PostCommentDTO" alias="PostCommentDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.ChannelDTO" alias="ChannelDTO"></typeAlias>
		<typeAlias type="com.jiyong.yt.dto.FavorTagDTO" alias="FavorTagDTO"></typeAlias>
	</typeAliases>
	
	
	<mappers>
		<mapper resource="mappers/TagMapper.xml" />
		<mapper resource="mappers/PostMapper.xml" />
		<mapper resource="mappers/UserMapper.xml" />
		<mapper resource="mappers/ChannelMapper.xml" />
	</mappers>
</configuration>
